@{
    ViewBag.Title = "Transferee Page";
    Layout = "~/Views/Shared/_Common.cshtml";
}

    <header class="container-fluid">
        <nav id="transfereeSideNav">
            <div class="transfereeSideNav__person">
                <img class="person" src="~/Content/Images/icn_person.png"/>
                <p class="eeName">Victoria A. Sporer</p>
                <p>Domestic | VIP</p>
                <p>Acme</p>
                <p>Road Runner LLC.</p>
            </div>
            <hr class="transfereeSideNav__rule"/>
            <ul class="transfereeSideNav__items">
                <li class="item selected" data-panel="intake">
                    <img src="~/Content/Images/icn_intake.png" />
                    <label>Intake</label>
                </li>
                <li class="item" data-panel="details">
                    <img src="~/Content/Images/icn_details.png" />
                    <label>Details</label>
                </li>
                <li class="item" data-panel="housing">
                    <img src="~/Content/Images/icn_housing.png" />
                    <label>Housing</label>
                </li>
                <li class="item" data-panel="history">
                    <img src="~/Content/Images/icn_history.png" />
                    <label>History</label>
                </li>
                <hr class="transfereeSideNav__rule" />
                <li class="item" data-panel="messages">
                    <img src="~/Content/Images/icn_internal_messages.png" />
                    <label>Messages</label>
                </li>
                <li class="item" data-panel="itinerary">
                    <img src="~/Content/Images/icn_internal_itinerary.png" />
                    <label>Itinerary</label>
                </li>
            </ul>
        </nav>
    </header>

    <div class="container" id="orderContainer">
        @Html.Partial("~/Views/Orders/Partials/_Intake.cshtml")
    </div>

@section scripts
{
    @Styles.Render("~/Styling/transferee")
    @Scripts.Render("~/Scripts/transferee")

    <script>
            var currentOrderId = '@Model.Id';

            $(document).ready(function () {
                OrdersPageController.TransfereeInit();
                TransfereeIntakeController.init();
                TransfereeDetailsController.init();
            });
    </script>
}