                else
                {
                    @Html.DatePicker("intake-date date", "TempHousingEndDate")
                }
                <input type="hidden" value="@Model.TempHousingEndDateDisplay" class="intake-hidden" name="TempHousingEndDate"/>
                
            </div>
        </div>
    </div>
