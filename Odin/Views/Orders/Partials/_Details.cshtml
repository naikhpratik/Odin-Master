@model Odin.ViewModels.Orders.Transferee.OrdersTransfereeViewModel

<div class="container-fluid" id="details" data-order-id="@Model.Id">
    <div class="row details-title">
        <p>Profile Summary</p>
        <span class="details-blue"></span>
    </div>
    <hr />
    <div class="details-block">
        <div class="row details-header">
            <P>
                Important Dates:
            </P>
        </div>
        <div class="row details-row">
            <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 col-xl-2 details-col">
                <label>Familiarization Trip:</label>
                <span>@Model.PreTripDateDisplay</span>
            </div>
            <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 col-xl-2 details-col">
                <label>Work Start Date:</label>
                <span>@Model.WorkStartDateDisplay</span>
            </div>
            <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 col-xl-2 details-col">
                <label>Temp Housing:</label>
                <span>@Model.TempHousingDays</span>
            </div>

            <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 col-xl-2 details-col">
                <label>Final Arrival:</label>
                <span>@Model.FinalArrivalDateDisplay</span>
            </div>
            <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 col-xl-2 details-col">
                <label>Home Finding Date:</label>
                <span>@Model.HomeFindingDateDisplay</span>
            </div>
            <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 col-xl-2 details-col">
                <label>Temp Housing Last Day:</label>
                <span>@Model.TempHousingEndDateDisplay</span>
            </div>
        </div>
    </div>
    @if (@Model.Rent != null)
    {
    <div class="details-block">
        <div class="row details-header">
            <P>
                Housing Details:
            </P>
        </div>
       <div class="row details-row">
        <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 col-xl-2 details-col">
            <label>Monthly Budget:</label>
            <span>@Model.HousingBudgetDisplay</span>
        </div>
        <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 col-xl-2 details-col">
            <label>Housing:</label>
            <span>@Model.HousingTypeDisplay</span>
        </div>
        <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 col-xl-2 details-col">
            <label>School District:</label>
            <span>@Model.SchoolDistrictDisplay </span>
        </div>

        <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 col-xl-2 details-col">
            <label>BD/BA:</label>
            <span>@Model.NumberOfBedroomsDisplay/@Model.NumberOfBathroomsDisplay</span>
        </div>
        <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 col-xl-2 details-col">
            <label>Sq.Ft:</label>
            <span>@Model.SquareFootageDisplay</span>
        </div>
        <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 col-xl-2 details-col">
            <label>Rent or Own:</label>
            <span>@Model.Rent.OwnershipType</span>
        </div>
    </div>
    </div>
    }
    <div class="details-block">
        <div class="row details-header">
            <P>
                Other Details:
            </P>
        </div>
        <div class="row details-row">
            <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 col-xl-2 details-col">
                <label>Manager:</label>
                <span>@Model.ProgramManager.FullName</span>
            </div>
            <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 col-xl-2 details-col">
                <label>Manager Email:</label>
                <span>@Model.ProgramManager.Email</span>
            </div>
            <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 col-xl-2 details-col">
                <label>Manager #:</label>
                <span> @Model.PhoneNumberDisplay</span>
            </div>
            <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 col-xl-2 details-col">
                <label>SE Number:</label>
                <span>@Model.SeContactUidDisplay</span>
            </div>
            <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 col-xl-2 details-col">
                <label>Client Number:</label>
                <span>@Model.ClientFileNumberDisplay</span>
            </div>
        </div>
    </div>

    <hr />
    <div class="details-blocks" data-block="services">
        <div class="row details-title">
            <p>
                Scheduled Services
                <span class="details-save">
                    - Save
                </span>
            </p>
            <span class="details-blue">Please fill out any items that are not already auto-generated that could aid in the help of your clients placement.</span>
        </div>
    
        <hr />
        <div class="details-block" id="scheduledServices">
            <div class="row details-row">
                <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 col-xl-2 details-col">
                    <H5>Service:</H5>
                </div>
                <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 col-xl-2 details-col">
                    <H5>Scheduled Date:</H5>
                </div>
                <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 col-xl-2 details-col">
                    <H5>Time:</H5>
                </div>
                <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 col-xl-2 details-col">
                    <H5>Completed Date:</H5>
                </div>
                
                <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 col-xl-2 details-col">
                    <H5>Notes:</H5>
                </div>
            </div>

                @for (int i = 0; i < Model.Services.Count(); i++)
                {
                    if (Model.Services.ElementAt(i).Selected)
                    {
                <div class="row details-row" data-entity-id="@Model.Services.ElementAt(i).Id" data-entity-collection="services">
                    <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 col-xl-2 details-col">
                        <label>@Model.Services.ElementAt(i).Name.</label>
                    </div>
                    <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 col-xl-2 details-col">
                        @Html.DatePicker("date", "ScheduledDate", Model.Services.ElementAt(i).ScheduledDateDisplay)
                    </div>
                    <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 col-xl-2 details-col">
                        @Html.DatePicker("time", "ScheduledTime", Model.Services.ElementAt(i).ScheduledTimeDisplay)
                        <div class="text-danger" style="display:none;" data-entity-id="@Model.Services.ElementAt(i).Id" data-valmsg-replace="true" ><div for="ScheduledTime">Schedule time is required.</div></div>
                    </div>
                    <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 col-xl-2 details-col">
                        @Html.DatePicker("date", "CompletedDate", Model.Services.ElementAt(i).CompletedDateDisplay)
                    </div>                        
                    <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 col-xl-2 details-col">
                        <p>@Html.TextAreaFor(s => @Model.Services.ElementAt(i).Notes, new { @class = "form-control intake-service-input" })</p>
                    </div>
                </div>
                    }
                }
           
        </div>
    
    </div>
</div>
@Styles.Render("~/Styling/details")

<script>
    $(document).ready(function () {        
        TransfereeDetailsController.init();
    });
</script>