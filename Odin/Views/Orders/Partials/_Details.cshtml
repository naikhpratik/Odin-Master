<div id="details">
    <div class="row details-title">
        <p>Profile Summary</p>
        <span class="details-blue">Please fill out any items that are not already auto-generated that could aid in the help of your clients placement.</span>
    </div>
    <hr />
    <div class="details-block">
        <div class="row details-header">
            <P>
                Important Dates:
            </P>
        </div>
        <div class="row details-row">
            <div class="col-sm-3 details-col">
                <label>Familiarization Trip:</label>
                <span>Model.Transferee.FTrip</span>               
            </div>
            <div class="col-sm-3 details-col">
                <label>Work Start Date:</label>
                <span>Model.Transferee.WorkStartDate</span>               
            </div>
            <div class="col-sm-3 details-col">
                <label>Temp Housing:</label>
                <span>Model.Transferee.TempHousing</span>               
            </div>
        
            <div class="col-sm-3 details-col">
                <label>Final Arrival:</label>
                <span>Model.Transferee.FinalArrival</span>
            </div>
            <div class="col-sm-3 details-col">
                <label>Home Finding Date:</label>
                <span>Model.Transferee.HFDate</span>
            </div>
            <div class="col-sm-3 details-col">
                <label>Temp Housing Last Day:</label>
                <span>Model.Transferee.THLD</span>
            </div>
        </div>
        <div class="details-block">
            <div class="row details-header">
                <P>
                    Housing Details:
                </P>
            </div>
            <div class="row details-row">
                <div class="col-sm-3 details-col">
                    <label>Monthly Budget:</label>
                    <span>Model.Transferee.MonthlyBudget</span>
                </div>
                <div class="col-sm-3 details-col">
                    <label>Housing:</label>
                    <span>Model.Transferee.Housing</span>
                </div>
                <div class="col-sm-3 details-col">
                    <label>School District:</label>
                    <span>Model.Transferee.SchoolDistrict</span>
                </div>
            
                <div class="col-sm-3 details-col">
                    <label>BD/BA:</label>
                    <span>Model.Transferee.BD/Model.Transferee.BA</span>
                </div>
                <div class="col-sm-3 details-col">
                    <label>Sq. Ft:</label>
                    <span>Model.Transferee.SqFt</span>
                </div>
                <div class="col-sm-3 details-col">
                    <label>Rent or Own:</label>
                    <span>Model.Transferee.RentorOwn</span>
                </div>
            </div>
        </div>
        <div class="details-block">
            <div class="row details-header">
                <P>
                    Other Details:
                </P>
            </div>
            <div class="row details-row">
                <div class="col-sm-3 details-col">
                    <label>Manager:</label>
                    <span>Model.Transferee.Manager</span>
                </div>
                <div class="col-sm-3 details-col">
                    <label>Manager Email:</label>
                    <span>Model.Transferee.MEmail</span>
                </div>
                <div class="col-sm-3 details-col">
                    <label>Manager #:</label>
                    <span>Model.Transferee.ManNum</span>
                </div>
                <div class="col-sm-3 details-col">
                    <label>SE Number:</label>
                    <span>Model.Transferee.SENum</span>
                </div>
                <div class="col-sm-3 details-col">
                    <label>Client Number:</label>
                    <span>Model.Transferee.ClientNum</span>
                </div>
            </div>
        </div>
    </div>
    <hr />
    <div class="row details-title">
        <p>Scheduled Services</p>
        <span class="details-blue">Please fill out any items that are not already auto-generated that could aid in the help of your clients placement.</span>
    </div>
    <hr />
    <div class="details-block">
        <div class="row details-row">
            <div class="col-sm-3 details-col">
                <label>Service:</label>                
            </div>
            <div class="col-sm-3 details-col">
                <label>Scheduled Date:</label>                
            </div>
            <div class="col-sm-3 details-col">
                <label>Completed Date:</label>                
            </div>
            <div class="col-sm-3 details-col">
                <label>Time:</label>
            </div>
            <div class="col-sm-3 details-col">
                <label>Notes:</label>
            </div>
        </div>
        <div class="row details-row">
            <div class="col-sm-3 details-col">                
                <label>Pre Arrival Consultation</label>
            </div>
            <div class="col-sm-3 details-col">
                @*@Html.DatePicker("svcPreArrival")
                @Html.DatePickerFor(m => m.BirthDate)
                @Html.ValidationMessageFor(m => m.BirthDate)*@


                @Html.DatePicker("date", "preArrivalSchDate", DateTime.Now)
            </div>
            <div class="col-sm-3 details-col">                
                @Html.DatePicker("date", "preArrivalComplDate", DateTime.Now)
            </div>
            <div class="col-sm-3 details-col">               
                @Html.DatePicker("time", "preArrivalTime", DateTime.Now)
            </div>
            <div class="col-sm-3 details-col">
                <p>Model.Transferee.Notes</p>
                <input type="text" class="form-control" />
            </div>
        </div>
        <div class="row details-row">
            <div class="col-sm-3 details-col">
                <label>Welcome Packet</label>
            </div>
            <div class="col-sm-3 details-col">
                @Html.DatePicker("date", "welSchDate", DateTime.Now)
            </div>
            <div class="col-sm-3 details-col">
                @Html.DatePicker("date", "welComplDate", DateTime.Now)
            </div>
            <div class="col-sm-3 details-col">
                @Html.DatePicker("time", "welSchTimee", DateTime.Now)
            </div>
            <div class="col-sm-3 details-col">
                <p>Model.Transferee.Notes</p>
                <input type="text" class="form-control" />
            </div>
        </div>
        <div class="row details-row">
            <div class="col-sm-3 details-col">
                <label>Area Orientation</label>
            </div>
            <div class="col-sm-3 details-col">
                @Html.DatePicker("date", "orientSchDate", DateTime.Now)
            </div>
            <div class="col-sm-3 details-col">
                @Html.DatePicker("date", "orientComplDate", DateTime.Now)
            </div>
            <div class="col-sm-3 details-col">
                @Html.DatePicker("time", "orientSchTime", DateTime.Now)
            </div>
            <div class="col-sm-3 details-col">
                <p>Model.Transferee.Notes</p>
                <input type="text" class="form-control" />
            </div>
        </div>
        <div class="row details-row">
            <div class="col-sm-3 details-col">
                <label>Accompanied Home Finding</label>
            </div>
            <div class="col-sm-3 details-col">
                @Html.DatePicker("date", "findingSchDate", DateTime.Now)
            </div>
            <div class="col-sm-3 details-col">
                @Html.DatePicker("date", "findingComplDate", DateTime.Now)
            </div>
            <div class="col-sm-3 details-col">
                @Html.DatePicker("time", "findingSchTime", DateTime.Now)
            </div>
            <div class="col-sm-3 details-col" style="width:32%; max-width: 500px;text-wrap:normal">
                <p>Model.Transferee NotesModel Transferee NotesModel Transferee NotesModel Transferee.NotesModel Transferee NotesModel Transferee.NotesModel Transferee.NotesModel Transferee.NotesModel Transferee.NotesModel Transferee.NotesModel Transferee.Notes</p>
                <input type="text" class="form-control" />
            </div>
        </div>
       </div>
    </div>

        @Styles.Render("~/Styling/details")


